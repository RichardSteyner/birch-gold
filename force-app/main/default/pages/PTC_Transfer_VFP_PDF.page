<apex:page standardController="IRA_Queue__c"
    extensions="PTC_Transfer_PDF_Controller"
    applyBodyTag="false"
    showHeader="false"
    sidebar="false"
    renderAs="pdf"
>
<head>
    <title>PTC Transfer</title>
    <style>
        @media print {
            html, body,input{margin: 0; padding: 0; font-family: Helvetica, sans-serif}
        }
        @page{
            margin: 30px 22px;
            margin-bottom: 50px;
            @bottom-left {
                content: element(footer);
            }
        }
        div.footer {
            padding-bottom: 30px;
            position: running(footer);
        }
        .nropage:before {
            content: counter(page);
        }
        .pages:after {
            content: counter(pages);
        } 
        .page{height: 100%; width: 100%; page-break-after: always;}
        .background{background-color:#007491; color: #fff}
        .title_header{
            font-size: 15pt;
            font-weight: 600;
            color: #808080;
        }
        .gray{color: #808080;}
        .form{font-size: 9pt; width: 100%}
        .form tr{ height: 28px;}
        .onlyradio tr{height: 24px;!important}
        .form tr td{padding: 0 5px;color: #000;}
        td.input_change{ 
            background-color: #F1F4FF; 
            border-top: 1px solid #A0A3AD;  
            border-left: 1px solid #A0A3AD;  
            border-right: 1px solid #E9EBF4; 
            border-bottom: 1px solid #E9EBF4;
            color: #5E616B;
        }
        td.input_change div{width: 100%;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;}
        tr.section_header{
            background-color: #007491;
            color: #fff;
            font-size: 13pt;
            height: 35px;
            padding-bottom: 5px;
        }
        tr.section_header td{
            padding-left: 10px;
            padding-top: 10px;
        }
        tr.section_header b{
            font-size: 12pt;
        }
        tr.line_blank{height:12px;}
        .radio{width:18px;}
        .check{width:16px;}
        table.mini p{margin:3px 0;text-align: justify;text-align-last: auto;}
        td.extend p{margin:10px 0;text-align: justify;text-align-last: auto;}
        a{color:#007491};
    </style>
</head>
<body>
    <apex:pageMessages id="showmsg"></apex:pageMessages>
    <apex:outputText rendered="{! !Errors }">
    <div class="footer">
        <table class="form">
            <tr align="center">
                <td width="90%"></td>
                <td class="input_change nropage"></td>
                <td> of </td>
                <td class="input_change pages"></td>
            </tr>
        </table>
    </div>

    <div class="page">
        <table>
            <tr style="height:950px" >
                <td valign="center" >
                    <table>
                        <tr>
                            <td width="15%" style="padding-right: 13px;"><img src="{!URLFOR($Resource.Logo_pdf)}" width="90%"/></td>
                            <td width="85%" class="background" align="justify" style="padding: 10px;">
                                <p>This page has intentionally been left blank. </p>
                                <p>The execute <b>Application</b> above can be sent to Preferred Trust Company as a scanned or faxed copy. The
                                    Transfer or Conversion Form and Pre-Consent E-Sign Authorization Disclosure Form below must be sent
                                    to Preferred Trust Company as an original.</p>
                            </td>
                        </tr>
                    </table>
                </td>
            </tr>
        </table>            
    </div>
    
    <div class="page">
        <table width="98%" style="margin-left: 5px;">   
            <tr>
                <td width="9%"><img src="{!URLFOR($Resource.Logo_pdf)+'?left'}" style="width: 100%;"/></td>
                <td width="51%" style="padding-left: 5px; font-size: 11pt;">
                    <br/><b>Client Services</b><span class="gray"> | 888.990.7892</span><br/>
                    <small class="gray"><b style="color: #000">E.</b> info@ptcemail.com<br/>
                    <b style="color: #000">F</b>. 702.946.0136<br/>
                    <b style="color: #000">W</b>. www.preferredtrustcompany.com<br/>
                    2140 E. Pebble Road, Suite 140<br/>
                    Las Vegas, NV 89123<br/></small>
                </td>
                <td valign="top" align="right" class="gray">
                    <span class="title_header">Transfer or Conversion Form</span><br/>
                    <span>Traditional | Roth | SEP | SIMPLE</span><br/>
                    <table class="form" width="100%" style="margin-top: 8px;">
                        <tr>
                            <td width="60%">PTC IRA Account Number</td>
                            <td width="40%" class="input_change"></td>
                        </tr>
                    </table>
                    
                </td>
            </tr>
        </table>
        <br/>
        <table width="100%">
            <tr class="section_header"><td><b>SECTION 1</b> | IRA Account Owner Information</td></tr>
            <tr class="line_blank"></tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="10%">First Name</td>
                            <td width="36%" class="input_change"><div>{!PrimaryObject.Contact__r.FirstName}</div></td>
                            <td width="4%">Initial</td>
                            <td width="5%" class="input_change"><div>{!PrimaryObject.Contact__r.MiddleName}</div></td>
                            <td width="10%">Last Name</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Contact__r.LastName}</div></td>
                        </tr>
                    </table>
                </td>
                
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="5%">SSN</td>
                            <td width="20%" class="input_change"><div>{!PrimaryObject.Contact__r.SSN__c}</div></td>
                            <td width="15%">DOB (MM/DD/YY)</td>
                            <td width="14%" class="input_change"><div> <apex:outputText value="{0, date, M'/'d'/'yyyy}"><apex:param value="{!PrimaryObject.Contact__r.D_O_B__c}" /></apex:outputText></div></td>
                            <td width="5%">Email</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Contact__r.Email}</div></td>
                        </tr>
                    </table>
                </td>
                
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="13%">Phone Number</td>
                            <td width="30%" class="input_change" align="center"><div>{!PrimaryObject.Contact__r.Phone}</div></td>
                            <td width="18%">Cell Phone Number</td>
                            <td width="" class="input_change" align="center"><div>{!PrimaryObject.Contact__r.MobilePhone}</div></td>
                        </tr>
                    </table>
                </td>
                
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="12%">Street Address</td>
                            <td width="30%" class="input_change"><div>{!PrimaryObject.Contact__r.MailingStreet}</div></td>
                            <td width="4%">City</td>
                            <td width="20%" class="input_change"><div>{!PrimaryObject.Contact__r.MailingCity}</div></td>
                            <td width="5%">State</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Contact__r.MailingState}</div></td>
                            <td width="8%">Zip Code</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Contact__r.MailingPostalCode}</div></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr class="line_blank"></tr>
            <tr class="section_header"><td><b>SECTION 2</b> | Account Type</td></tr>
            <tr><!-- fields --><!-- Radio -->
                <td>
                    <table class="form" style="width:82%!important">
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, iF(contains(PrimaryObject.Account_Type__c,'Traditional'),'radiotrue.png','radiofalse.png') )}" class="radio"/></td>
                                        <td>Traditional IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, iF(contains(PrimaryObject.Account_Type__c,'Roth'),'radiotrue.png','radiofalse.png') )}" class="radio"/></td>
                                        <td>Roth IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, iF(contains(PrimaryObject.Account_Type__c,'SEP'),'radiotrue.png','radiofalse.png') )}" class="radio"/></td>
                                        <td>SEP IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, iF(contains(PrimaryObject.Account_Type__c,'SIMPLE'),'radiotrue.png','radiofalse.png') )}" class="radio"/></td>
                                        <td>SIMPLE IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, iF(contains(PrimaryObject.Account_Type__c,'Inherited'),'radiotrue.png','radiofalse.png') )}" class="radio"/></td>
                                        <td>Inherited IRA</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </td>
                
            </tr>
            <tr class="section_header"><td><b>SECTION 3</b> | Transfer From Information </td></tr>
            <tr class="line_blank"></tr>
            <tr><!-- Text -->
                <td>
                   <span style="color:#007491;font-size:9pt;font-family:sans-serif;">Preferred Trust Company requires a photocopy of a <b><u>current statement</u></b> from the Custodian the funds are being transferred from.</span>
                </td>
            </tr>
            <tr class="line_blank"></tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="15%">Custodian Name</td>
                            <td width="45%" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Name}</div></td>
                            <td width="18%">Client Account Number</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Custodian_Client_Account_Number__c}</div></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="14%">Street Address</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Account.BillingStreet}</div></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form">
                        <tr>
                            <td width="4%">City</td>
                            <td width="60%" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Account.BillingCity}</div></td>
                            <td width="5%">State</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Account.BillingState}</div></td>
                            <td width="8%">Zip Code</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Account.BillingPostalCode}</div></td>
                        </tr>
                    </table>
                </td>
            </tr>
            <tr><!-- fields -->
                <td>
                    <table class="form" style="width:50%!important">
                        <tr>
                            <td width="27%">Phone Number</td>
                            <td width="" class="input_change"><div>{!PrimaryObject.Custodian_name__r.Phone}</div></td>
                        </tr>
                    </table>
                </td>   
            </tr>
            <tr class="line_blank"></tr>      
            <tr class="section_header"><td><b>SECTION 4</b> | Type of Transaction <span style="font-size: 8pt;">(See Additional Information included with this form.)</span></td></tr>
            <tr class="line_blank"></tr>
            <tr><!-- text -->
                <td>
                    <table style="width:75%!important">
                        <tr>
                            <td width="30%" style="font-size:8pt;">A. <b>Transfer</b>: (check one)</td>
                            <td width=""  style="font-size:8pt;">
                                <b>[SIMPLE IRA cannot be transferred to a Traditional IRA or converted to a<br/>
                                Roth IRA for 2 years following the date of the initial SIMPLE contribution.]</b>
                            </td>
                        </tr>
                    </table>
                </td>   
            </tr>
            <tr><!-- fields --> <!-- check -->
                <td>
                    <table class="form" style="width:87%;font-size:8pt;">
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'Traditional IRA to a Traditional IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>Traditional IRA to a Traditional IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'Traditional IRA to a SEP IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>Traditional IRA to a SEP IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'Roth IRA to a Roth IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>Roth IRA to a Roth IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'SEP IRA to a SEP IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SEP IRA to a SEP IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'SEP IRA to a Traditional IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SEP IRA to a Traditional IRA</td>
                                    </tr>
                                </table>
                            </td>                        
                        </tr>
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'SIMPLE IRA to a SEP IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SIMPLE IRA to a SEP IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'SIMPLE IRA to a Traditional IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SIMPLE IRA to a Traditional IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'SIMPLE IRA to a SIMPLE IRA'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SIMPLE IRA to a SIMPLE IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center" colspan="2">
                                <table width="100%">
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Transfer__c,'Other'),'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>Other</td>
                                        <td width="80%" class="input_change"><div>{!if(contains(PrimaryObject.Transfer__c,'Other') , PrimaryObject.Other_Transfer__c, '')}</div></td>
                                    </tr>
                                </table>
                            </td>                        
                        </tr>
                    </table>
                </td>   
            </tr>
            <tr class="line_blank"></tr>
            <tr><!-- text -->
                <td>
                    <table style="width:75%!important">
                        <tr>
                            <td width="30%" style="font-size:8pt;">A. <b>Conversion</b>: (check one)</td>
                            <td width=""  style="font-size:8pt;">
                                <b>[Required Minimum Distributions (RMD) cannot be converted to a Roth IRA.]</b>
                            </td>
                        </tr>
                    </table>
                </td>   
            </tr>
            <tr><!-- fields --> <!-- check -->
                <td>
                    <table class="form" style="font-size:8pt;width:87%">
                        <tr>
                            <td valign="center" width="22%">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Conversion__c,'SIMPLE IRA to a Roth IRA') ,'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SIMPLE IRA to a Roth IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center" width="22%">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Conversion__c,'Traditional IRA to a Roth IRA') ,'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>Traditional IRA to a Roth IRA</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center" width="22%">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Conversion__c,'SEP IRA to a Roth IRA') ,'checktrue.png' , 'checkfalse.png') )}" class="check"/></td>
                                        <td>SEP IRA to a Roth IRA</td>
                                    </tr>
                                </table>
                            </td>
                            <td valign="center" width="25%"></td>
                            <td valign="center" width="25%"></td>
                        </tr>
                    </table>
                    <table style="font-size:8pt;">
                        <tr>                          
                            <td valign="center" width="40%">
                                <table>
                                    <tr valign="center">
                                        <td width="81.5%">If applicable, the required minimum distribution</td>
                                        <td><img src="{!URLFOR($Resource.helper_images, if(PrimaryObject.Required_Minimum_Distribution_has__c ,'checktrue.png', 'checkfalse.png') )}" class="check"/></td>
                                        <td>has or</td>
                                    </tr>
                                </table>
                            </td>                            
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(PrimaryObject.Required_Minimum_Distribution_has_not__c ,'checktrue.png', 'checkfalse.png') )}" class="check"/></td>
                                        <td>has not been satisfied for this distribution year prior to this transfer. If not, the transferee custodian/trustee may require additional documentation.</td>
                                    </tr>
                                </table>
                            </td>                        
                        </tr>
                    </table>
                </td>   
            </tr>
        </table>
    </div>

    <div class="page">
        <table width="100%">
            <tr class="section_header"><td><b>SECTION 5</b> | Transfer or Conversion Instructions</td></tr>
            <tr><!-- Text -->
                <td>
                   <span style="color:#007491;font-size:8.5pt;font-family:sans-serif;">Complete my transfer/conversion as directed. Note: Penalties and market fluctuation may affect the distribution amount of investments held by your IRA.</span>
                </td>
            </tr>
            <tr>
                <td>
                    <table width="45%">
                        <tr style="height:25px;">
                            <td valign="center" style="background-color:#C0C0C0;padding:5px;"><span style="color:#007491;font-size:9pt;font-family:sans-serif;"><b>CASH</b> | You must select an option from this section.</span></td>
                        </tr>
                    </table>
                </td>
            </tr>            
            <tr><!-- fields --><!-- Radio -->
                <td>
                    <table class="form" style="width:60%!important">
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Cash_transfer__c,'My entire cash balance'), 'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>My entire cash balance only.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td valign="center">
                                <table> 
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Cash_transfer__c,'A portion of my cash balance'), 'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>A portion of my cash balance totaling:</td>
                                        <td width="45%" class="input_change"><div>{!if(contains(PrimaryObject.Cash_transfer__c,'A portion of my cash balance'),PrimaryObject.Portion_of_cash_balance_Total__c, '' ) }</div></td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Cash_transfer__c,'My entire cash balance and investments as described below'), 'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>My entire cash balance <b>and</b> the investments as described below.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                    </table>
                </td>                
            </tr>
            <tr><!-- Text -->
                <td>
                    <table width="45%">
                        <tr style="height:25px;">
                            <td valign="center" style="background-color:#C0C0C0;padding:5px;"><span style="color:#007491;font-size:9pt;font-family:sans-serif;"><b>INVESTMENTS</b> | You must select an option from this section.</span></td>
                        </tr>
                    </table>
                </td>
            </tr>            
            <tr><!-- fields --><!-- Radio -->
                <td>
                    <table class="form onlyradio">
                        <tr>
                            <td valign="center">
                                <table>
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Investments__c,'No investment liquidation required'),'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>No investment liquidation required.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td valign="center">
                                <table> 
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Investments__c,'Immediately liquidate investments and send cash proceeds'),'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>Immediately liquidate investments and send cash proceeds.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td valign="center">
                                <table> 
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Investments__c,'Send all investments in-kind'),'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>Send all investments in-kind.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td valign="center">
                                <table> 
                                    <tr valign="center">
                                        <td><img src="{!URLFOR($Resource.helper_images, if(contains(PrimaryObject.Investments__c,'Send only the identified investments below in kind'),'radiotrue.png' ,'radiofalse.png') )}" class="radio"/></td>
                                        <td>Send only the identified investment(s) below in-kind.</td>
                                    </tr>
                                </table>
                            </td>
                        </tr>
                        <tr>
                            <td>
                                <table class="form">
                                    <tr>
                                        <td width="18%">Investment Name <b>1</b></td>
                                        <td width="" class="input_change"><div>{!PrimaryObject.Investment_Name_1__c}</div></td>
                                    </tr>
                                </table>
                            </td>  
                        </tr>
                        <tr>
